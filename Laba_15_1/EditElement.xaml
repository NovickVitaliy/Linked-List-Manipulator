<Window x:Class="Laba_15_1.EditElement"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Laba_15_1"
        mc:Ignorable="d"
        Title="EditElement" Height="450" Width="800">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Orientation="Vertical">
    <Label>Index</Label>
    <TextBox x:Name="tbIndex"></TextBox>
    <Label>Value</Label>
    <TextBox x:Name="tbValue"></TextBox>
    <Button x:Name="EditElementButton" Click="EditElementButton_Click">Edit element</Button>
    </StackPanel>
</Window>
